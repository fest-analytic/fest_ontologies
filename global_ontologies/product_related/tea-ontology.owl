<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.tea.com//ontologies/tea.owl#"
     xml:base="http://www.tea.com//ontologies/tea.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.tea.com//ontologies/tea.owl">
        <rdfs:comment>A tea ontology that describes various kinds of tea.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.tea.com//ontologies/tea.owl#hasAdditionalIngredient -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#hasAdditionalIngredient">
        <rdfs:subPropertyOf rdf:resource="http://www.tea.com//ontologies/tea.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isAdditionalIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
        <rdfs:range rdf:resource="http://www.tea.com//ontologies/tea.owl#Ingredient"/>
        <rdfs:comment>Имеет дополнительный ингредиент (мед, сахар и т.д.)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#hasFlavourAddition -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition">
        <rdfs:subPropertyOf rdf:resource="http://www.tea.com//ontologies/tea.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isFlavourAdditionOf"/>
        <rdfs:domain rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
        <rdfs:range rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaFlavourAddition"/>
        <rdfs:comment>Имеет вкусовую добавку</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>Имеет ингредиент</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#hasTeaBase -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#hasTeaBase">
        <rdfs:subPropertyOf rdf:resource="http://www.tea.com//ontologies/tea.owl#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isTeaBaseOf"/>
        <rdfs:domain rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
        <rdfs:range rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaType"/>
        <rdfs:comment>Имеет основу на таком-то чае (черном, зеленом и т.д.)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#isAdditionalIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#isAdditionalIngredientOf">
        <rdfs:subPropertyOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.tea.com//ontologies/tea.owl#Ingredient"/>
        <rdfs:range rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#isFlavourAdditionOf -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#isFlavourAdditionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaFlavourAddition"/>
        <rdfs:range rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.tea.com//ontologies/tea.owl#isTeaBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.tea.com//ontologies/tea.owl#isTeaBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.tea.com//ontologies/tea.owl#isIngredientOf"/>
        <rdfs:domain rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaType"/>
        <rdfs:range rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.tea.com//ontologies/tea.owl#hasTeaPortion -->

    <owl:DatatypeProperty rdf:about="http://www.tea.com//ontologies/tea.owl#hasTeaPortion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Сколько условных порций чая кладут при заварке (более крепкая или слабая заварка).
В травяных чаях (чайных напитках), где нет чайных листьев, такая порция равна 0.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.tea.com//ontologies/tea.owl#Anise -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Anise">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#Spice"/>
        <rdfs:comment>Анис</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Apple -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Apple">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Яблоко</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Barberry -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Barberry">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Барбарис</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Bergamot -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Bergamot">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Бергамот</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Black -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Black">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#Oxidized"/>
        <rdfs:comment xml:lang="en">Черный чай</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#BlackTea -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#BlackTea">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#NamedTeaDrink"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                        <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaFlavourAddition"/>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaBase"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Black"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaBase"/>
                <owl:allValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Black"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaPortion"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Черный чай без добавок</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#BlackTeaDrink -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#BlackTeaDrink">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaBase"/>
                        <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Black"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.tea.com//ontologies/tea.owl#GreenTeaDrink"/>
        <owl:disjointWith rdf:resource="http://www.tea.com//ontologies/tea.owl#HerbalTeaDrink"/>
        <rdfs:comment>Чаи, в составе которых есть черный чай</rdfs:comment>
        <rdfs:label xml:lang="en">BlackTeaDrink</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#BlackTeaWithBerries -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#BlackTeaWithBerries">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#NamedTeaDrink"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Blueberry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Raspberry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Strawberry"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaBase"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Black"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaPortion"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Черный чай с ягодами</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Blackberry -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Blackberry">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Ежевика</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Blueberry -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Blueberry">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Черника</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Cactus -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Cactus">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Цветки кактуса</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Cardamom -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Cardamom">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#Spice"/>
        <rdfs:comment>Кардамон</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Chamomile -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Chamomile">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Ромашка</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Cinnamon -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Cinnamon">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#Spice"/>
        <rdfs:comment>Корица</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Cloves -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Cloves">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#Spice"/>
        <rdfs:comment>Гвоздика</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Cornflower -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Cornflower">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Василек</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Cranberry -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Cranberry">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Клюква</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Currant -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Currant">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Смородина</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#CurrantLeaves -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#CurrantLeaves">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Листья смородины</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Dandelion -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Dandelion">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Маргаритка</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#DryLemon -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#DryLemon">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
        <rdfs:comment>Сушеный лимон</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#EarlGrey -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#EarlGrey">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#NamedTeaDrink"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Bergamot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaBase"/>
                <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Black"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                <owl:allValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Bergamot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaBase"/>
                <owl:allValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Black"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasTeaPortion"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Черный чай с добавлением бергамота</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Eucalyptus -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#Eucalyptus">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
        <rdfs:comment>Эвкалипт</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#FlavouredTea -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#FlavouredTea">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.tea.com//ontologies/tea.owl#TeaDrink"/>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                                        <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                                        <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#FruitAndBerry"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.tea.com//ontologies/tea.owl#hasFlavourAddition"/>
                                        <owl:someValuesFrom rdf:resource="http://www.tea.com//ontologies/tea.owl#Spice"/>
                                    </owl:Restriction>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.tea.com//ontologies/tea.owl#PureTea"/>
        <rdfs:comment>Чай с какими-либо добавками - цветочными, ягодными, фруктовыми или c пряностями.</rdfs:comment>
        <rdfs:label xml:lang="en">FlavouredTea</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#FlowerAndHerbs">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaFlavourAddition"/>
        <rdfs:comment>Цветы и травы</rdfs:comment>
        <rdfs:label xml:lang="en">FlowerAndHerbs</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#FruitAndBerry -->

    <owl:Class rdf:about="http://www.tea.com//ontologies/tea.owl#FruitAndBerry">
        <rdfs:subClassOf rdf:resource="http://www.tea.com//ontologies/tea.owl#TeaFlavourAddition"/>
        <rdfs:comment>Ягоды и фрукты</rdfs:comment>
        <rdfs:label xml:lang="en">FruitAndBerry</rdfs:label>
    </owl:Class>
    


    <!-- http://www.tea.com//ontologies/tea.owl#Ginger -->

